{
  "name": "react-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "build": "next build",
    "lint": "next lint",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run lint"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@prisma/client": "^4.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "next": "12.1.6",
    "prettier": "^2.6.2",
    "prisma": "^4.1.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "sass": "^1.54.0"
  },
  "devDependencies": {
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "eslint": "^8.16.0",
    "eslint-config-next": "12.1.6",
    "husky": "^7.0.0",
    "typescript": "4.6.4"
  }
}
